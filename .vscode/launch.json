    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'boowebserver'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=boowebserver"
                ],
                "filter": {
                    "name": "boowebserver",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'boowebserver'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=boowebserver",
                    "--package=boowebserver"
                ],
                "filter": {
                    "name": "boowebserver",
                    "kind": "bin"
                }
            },
            "args": ["start", "-ip", "127.0.0.1", "-p", "7878", "-tp", "10"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'boowebserver'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=boowebserver",
                    "--package=boowebserver"
                ],
                "filter": {
                    "name": "boowebserver",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}